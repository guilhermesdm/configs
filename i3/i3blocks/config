[backlight]
command=$SCRIPT_DIR/backlight.sh
label=☀
interval=once
signal=10

[battery]
command=$SCRIPT_DIR/battery
interval=30
LABEL=⚡

[temp]
command=$SCRIPT_DIR/temp.sh
interval=5

[volume-pipewire]
command=$SCRIPT_DIR/volume
interval=persist
SUBSCRIBE=1
USE_ALSA_NAME=1

[keyboard]
command=$SCRIPT_DIR/kb-status.sh
interval=2

[time]
command=date '+%H:%M - %d/%m/%Y'
interval=5

[dunst]
command=$SCRIPT_DIR/dunst
interval=once
format=json
markup=pango
